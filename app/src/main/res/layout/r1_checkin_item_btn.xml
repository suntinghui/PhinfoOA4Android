<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="fill_parent"    android:layout_height="wrap_content"    android:background="@drawable/my_item_selector"    android:gravity="center_vertical"    android:orientation="horizontal"    android:paddingBottom="0dp"    android:paddingTop="0dp" >    <RelativeLayout        android:layout_width="wrap_content"        android:layout_height="fill_parent"        android:gravity="center_horizontal" >        <TextView            android:layout_width="1dp"            android:layout_height="fill_parent"            android:layout_centerHorizontal="true"            android:background="#f2f2f2" />        <TextView            android:id="@+id/date"            android:layout_width="30dp"            android:layout_height="30dp"            android:layout_centerHorizontal="true"            android:layout_marginTop="10dp"            android:gravity="center"            android:textColor="#ffffff"            android:textSize="14sp" />    </RelativeLayout>    <LinearLayout        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_marginLeft="10dp"        android:orientation="vertical"        android:paddingBottom="10dp"        android:paddingTop="10dp" >        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:orientation="horizontal" >            <TextView                android:id="@+id/title"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:gravity="left|top"                android:textColor="#333333"                android:textSize="16sp" />            <TextView                android:id="@+id/title1"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="10dp"                android:gravity="left|top"                android:textColor="#666666"                android:textSize="12sp" />        </LinearLayout>        <TextView            android:id="@+id/address"            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="4dp"            android:drawableLeft="@drawable/ic_pos"            android:gravity="left|top"            android:textColor="#666666"            android:textSize="14sp"            android:visibility="gone" />        <TextView            android:id="@+id/wifi"            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="4dp"            android:drawableLeft="@drawable/wifi"            android:gravity="left|top"            android:textColor="#666666"            android:textSize="14sp"            android:visibility="gone" />        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="4dp"            android:orientation="horizontal" >            <TextView                android:id="@+id/outBag"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginRight="10dp"                android:background="@drawable/rounded_blue_n"                android:gravity="center"                android:paddingBottom="2dp"                android:paddingLeft="10dp"                android:paddingRight="10dp"                android:paddingTop="2dp"                android:text="外勤"                android:textColor="#ffffff"                android:textSize="12sp"                android:visibility="gone" />            <TextView                android:id="@+id/laterBag"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:background="@drawable/rounded_orange_n"                android:gravity="center"                android:paddingBottom="2dp"                android:paddingLeft="10dp"                android:paddingRight="10dp"                android:paddingTop="2dp"                android:text="迟到"                android:textColor="#ffffff"                android:textSize="12sp"                android:visibility="gone" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="10dp"            android:layout_marginTop="4dp"            android:orientation="horizontal" >            <TextView                android:id="@+id/updateReplacement"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:background="@drawable/rounded_blue_frame_n"                android:drawablePadding="5dp"                android:drawableRight="@drawable/arrow"                android:gravity="center"                android:paddingBottom="2dp"                android:paddingLeft="10dp"                android:paddingRight="10dp"                android:paddingTop="2dp"                android:text="申请补卡"                android:textColor="#ff40b1eb"                android:textSize="14sp"                android:visibility="gone" />            <TextView                android:id="@+id/updateAddress"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:background="@drawable/rounded_blue_frame_n"                android:drawablePadding="5dp"                android:drawableRight="@drawable/arrow"                android:gravity="center"                android:paddingBottom="2dp"                android:paddingLeft="10dp"                android:paddingRight="10dp"                android:paddingTop="2dp"                android:text="更新打卡"                android:textColor="#ff40b1eb"                android:textSize="14sp"                android:visibility="gone" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:gravity="center_horizontal"            android:orientation="vertical" >            <View                android:id="@+id/emptypace"                android:layout_width="fill_parent"                android:layout_height="80dp"                android:visibility="gone" />            <LinearLayout                android:id="@+id/CheckInBtn"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:minHeight="120dp"                android:minWidth="120dp"                android:layout_marginTop="10dp"                android:background="@drawable/oval_55bdf2_selector"                android:gravity="center"                android:orientation="vertical"                android:visibility="gone" >                <TextView                    android:id="@+id/CheckText"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="上班打卡"                    android:textColor="#ffffff"                    android:textSize="18dp" />                <TextView                    android:id="@+id/CheckDateTime"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:textColor="#ffffff"                    android:textSize="14dp" />            </LinearLayout>            <TextView                android:id="@+id/tip"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginTop="4dp"                android:text="当前不在考勤范围"                android:textColor="#333333"                android:textSize="12sp"                android:visibility="gone" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:gravity="center_horizontal" >            <com.heqifuhou.view.RadarView                android:id="@+id/radarBtn"                android:layout_width="140dp"                android:layout_height="140dp"                android:visibility="gone"                android:layout_marginTop="10dp" />        </LinearLayout>    </LinearLayout></LinearLayout>